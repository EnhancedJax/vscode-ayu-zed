{
  "name": "vscode-ayu-zed",
  "displayName": "Ayu Mirage Zed",
  "description": "A remake of Zed's Ayu mirage theme for VSCode.",
  "version": "1.0.0",
  "publisher": "enhanced-jax",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/enhanced-jax/vscode-ayu-zed"
  },
  "bugs": {
    "url": "https://github.com/enhanced-jax/vscode-ayu-zed/issues",
    "email": "enhancedjax@gmail.com"
  },
  "engines": {
    "vscode": "^1.13.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "icon-theme",
    "theme",
    "ayu"
  ],
  "homepage": "https://github.com/enhanced-jax/vscode-ayu-zed",
  "icon": "assets/logo.png",
  "contributes": {
    "themes": [
      {
        "label": "Ayu Mirage Zed",
        "uiTheme": "vs-dark",
        "path": "./ayu-mirage-zed.json"
      }
    ],
    "iconThemes": [
      {
        "id": "ayu-mirage-zed",
        "label": "Ayu Mirage Zed",
        "path": "./ayu-mirage-zed-icons.json"
      }
    ]
  },
  "scripts": {
    "build": "ts-node ./src/build.ts",
    "package": "vsce package",
    "start": "npm run build; npm run package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^16.6.0",
    "ayu": "^8.0.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3",
    "vsce": "^1.103.1"
  }
}
